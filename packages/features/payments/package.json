{
  "name": "@nyoworks/feature-payments",
  "version": "2.6.0",
  "description": "NYOWORKS Payments Feature - Stripe integration",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./schema": {
      "import": "./dist/schema.js",
      "types": "./dist/schema.d.ts"
    },
    "./router": {
      "import": "./dist/router.js",
      "types": "./dist/router.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/schema.ts src/router.ts --format esm --dts",
    "dev": "tsup src/index.ts src/schema.ts src/router.ts --format esm --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "stripe": "^18.0.0"
  },
  "peerDependencies": {
    "@trpc/server": "^11.0.0",
    "drizzle-orm": "^0.38.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.7.0"
  }
}

{
  "name": "@nyoworks/feature-crm",
  "version": "2.6.0",
  "description": "CRM feature for NYOWORKS projects - contacts, notes, tags management",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./schema": {
      "import": "./src/schema.ts",
      "types": "./src/schema.ts"
    },
    "./router": {
      "import": "./src/router.ts",
      "types": "./src/router.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "drizzle-orm": "^0.44.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@trpc/server": "^11.0.0",
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "@trpc/server": "^11.0.0",
    "react": "^19.0.0"
  }
}

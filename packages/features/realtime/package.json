{
  "name": "@nyoworks/feature-realtime",
  "version": "2.7.0",
  "description": "NYOWORKS Realtime Feature - WebSocket and presence management",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js"
    },
    "./router": {
      "types": "./dist/router.d.ts",
      "import": "./dist/router.js"
    },
    "./websocket": {
      "types": "./dist/websocket.d.ts",
      "import": "./dist/websocket.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/schema.ts src/router.ts src/websocket.ts --format esm --dts --external react --external '@nyoworks/*'",
    "dev": "tsup src/index.ts src/schema.ts src/router.ts src/websocket.ts --format esm --dts --external react --external '@nyoworks/*' --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nyoworks/shared": "workspace:*",
    "@paralleldrive/cuid2": "^3.3.0",
    "ws": "^8.19.0"
  },
  "peerDependencies": {
    "@nyoworks/api": "workspace:*",
    "@nyoworks/database": "workspace:*",
    "@trpc/server": "^11.0.0",
    "drizzle-orm": "^0.45.0",
    "react": "^19.0.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "@types/react": "^19.2.13",
    "@types/ws": "^8.18.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}

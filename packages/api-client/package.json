{
  "name": "@nyoworks/api-client",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./vanilla": {
      "import": "./dist/vanilla.js",
      "types": "./dist/vanilla.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/react.tsx src/vanilla.ts --format esm --dts",
    "dev": "tsup src/index.ts src/react.tsx src/vanilla.ts --format esm --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@tanstack/react-query": "^5.62.0",
    "superjson": "^2.2.1"
  },
  "devDependencies": {
    "@nyoworks/api": "workspace:*",
    "@types/node": "^22.15.0",
    "@types/react": "^19.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "react": "^19.0.0"
  },
  "peerDependencies": {
    "@nyoworks/api": "workspace:*",
    "react": "^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}

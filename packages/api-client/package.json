{
  "name": "@nyoworks/api-client",
  "version": "2.6.1",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js"
    },
    "./vanilla": {
      "types": "./dist/vanilla.d.ts",
      "import": "./dist/vanilla.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/react.tsx src/vanilla.ts --format esm --dts",
    "dev": "tsup src/index.ts src/react.tsx src/vanilla.ts --format esm --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.20",
    "@trpc/client": "^11.9.0",
    "@trpc/react-query": "^11.9.0",
    "superjson": "^2.2.6"
  },
  "devDependencies": {
    "@nyoworks/api": "workspace:*",
    "@types/node": "^25.2.2",
    "@types/react": "^19.2.13",
    "react": "^19.2.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@nyoworks/api": "workspace:*",
    "react": "^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
